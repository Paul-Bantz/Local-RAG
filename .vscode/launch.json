{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python:Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "env": {
                "TOKENIZERS_PARALLELISM": "true",
                "TAVILY_API_KEY": "<TAVILY_API_KEY>",
                "LLM_HOST": "<LLM_HOST>",
                "LLM_MODEL": "<LLM_MODEL>"
            },
            "args": [
                "run",
                "${workspaceFolder}/LocalRAG/streamlit_app.py",
                "--server.port",
                "2000"
            ]
        }

    ]
}